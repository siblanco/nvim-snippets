{
  "name": "nvim-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "css",
          "scss",
          "sass",
          "less",
          "stylus"
        ],
        "path": "./snippets/scss.json"
      },
      {
        "language": [
          "html",
          "php",
          "twig"
        ],
        "path": "./snippets/html.json"
      },
      {
        "language": [
          "javascript",
          "vue",
          "svelte",
          "typescript"
        ],
        "path": "./snippets/typescript.json"
      },
      {
        "language": [
          "typescriptreact",
          "javascriptreact"
        ],
        "path": "./snippets/typescriptreact.json"
      }
    ]
  }
}
